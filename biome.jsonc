{
  "$schema": "https://biomejs.dev/schemas/2.1.3/schema.json",
  "extends": ["ultracite"],
  "overrides": [
    {
      "includes": ["src/generated/**"],
      "linter": {
        "enabled": false
      },
      "formatter": {
        "enabled": false
      }
    }
  ],
  "linter": {
    "rules": {
      "suspicious": {
        "noConsole": "off"
      },
      "style": {
        "useBlockStatements": "off"
      }
    }
  },
  "formatter": {
    "lineWidth": 120
  }
}
